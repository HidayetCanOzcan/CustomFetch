{
    "name": "@hidayetcanozcan/custom-fetch",
    "version": "1.0.0",
    "description": "Production-ready, 100% type-safe HTTP client for Node.js and Bun server-side environments. Zero dependencies, discriminated union responses, automatic retries, interceptors, caching, runtime validation, and more.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rm -rf dist",
        "prepare": "npm run build",
        "prepublishOnly": "npm run clean && npm run build",
        "release": "npm publish",
        "release:dry": "npm publish --dry-run"
    },
    "keywords": [
        "fetch",
        "http",
        "http-client",
        "api-client",
        "rest",
        "rest-client",
        "typescript",
        "type-safe",
        "node",
        "nodejs",
        "bun",
        "server-side",
        "discriminated-union",
        "retry",
        "exponential-backoff",
        "interceptors",
        "cache",
        "caching",
        "timeout",
        "abort-controller",
        "runtime-validation",
        "type-guard",
        "zero-dependencies"
    ],
    "author": {
        "name": "Hidayet Can Ã–zcan",
        "email": "hidayetcan@gmail.com",
        "url": "https://github.com/HidayetCanOzcan"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HidayetCanOzcan/CustomFetch.git"
    },
    "bugs": {
        "url": "https://github.com/HidayetCanOzcan/CustomFetch/issues",
        "email": "hidayetcan@gmail.com"
    },
    "homepage": "https://github.com/HidayetCanOzcan/CustomFetch#readme",
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/HidayetCanOzcan"
    },
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "bun": ">=1.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "typescript": "^5.7.0"
    }
}